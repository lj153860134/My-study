<?xml version="1.0" encoding="UTF-8"?>
<mapper namespace="com.lingjian_1.dao.IUserDao">
    <!--配置查询所有-->
    <select id="findAll" resultType="com.lingjian_1.domain.User">
        select * from user where id between 42 and 45
    </select>
    <!--保存用户-->
    <insert id="saveUser" parameterType="com.lingjian_1.domain.User">
        insert into user(username,address,sex,birthday) values(#{username},#{address},
        #{sex},#{birthday})
    </insert>

</mapper>